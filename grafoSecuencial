class GrafoSecuencial:
    def __init__(self, vertices):
        self.vertices = vertices
        self.matriz_adyacencia = [[0] * vertices for _ in range(vertices)]

    def agregar_arista(self, inicio, fin):
        if 0 <= inicio < self.vertices and 0 <= fin < self.vertices:
            self.matriz_adyacencia[inicio][fin] = 1
            self.matriz_adyacencia[fin][inicio] = 1

    def mostrar_matriz_adyacencia(self):
        for fila in self.matriz_adyacencia:
            print(fila)

# Crear un grafo con 5 vÃ©rtices
grafo = GrafoSecuencial(5)

# Agregar aristas
grafo.agregar_arista(0, 1)
grafo.agregar_arista(0, 3)
grafo.agregar_arista(1, 4)
grafo.agregar_arista(2, 4)
grafo.agregar_arista(3, 4)

# Mostrar la matriz de adyacencia
print("Matriz de Adyacencia:")
grafo.mostrar_matriz_adyacencia()
